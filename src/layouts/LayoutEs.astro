---
import BaseHead from '../components/BaseHead.astro';
import Header from '../components/HeaderEs.astro';
import Footer from '../components/FooterEs.astro';


const { title, description } = Astro.props;
---

<html lang="es">
	<head>
		<BaseHead title={title} description={description} />
	</head>

	<body>
		<Header />
		<main>
			<slot />
		</main>
		<Footer />
	</body>
</html>
